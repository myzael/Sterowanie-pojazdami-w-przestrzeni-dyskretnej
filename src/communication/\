import httplib
from server import State

def printText(txt):
    lines = txt.split('\n')
    for line in lines:
        print line.strip()

httpServ = httplib.HTTPConnection("127.0.0.1", 8000)
httpServ.connect()

s = State()
s.allowedMoves.append((1,1))
s.setOwnPosition((0,0))
s.agents.append((0,0))

httpServ.request('POST', "/test", "ala ma kota")

response = httpServ.getresponse()
if response.status == httplib.OK:
	print "OK"

httpServ.close()
